@model IEnumerable<Employee>

<html>
<head>
    <title></title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Department</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <td>
                        @employee.id
                    </td>
                    <td>
                        @employee.name
                    </td>
                    <td>
                        @employee.Department
                    </td>
                </tr>
            }
        </tbody>

    </table>

    <div>
        <a asp-action="AddEmployee"> ADD MORE EMPLOYEE </a>
    </div>
</body>
</html>